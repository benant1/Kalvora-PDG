{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["prisma/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "build": {
    "env": {
      "PRISMA_GENERATE_FLAG": "true"
    }
  },
  "env": {
    "PRISMA_GENERATE_FLAG": "true"
  },
  "buildCommand": "prisma generate && prisma migrate deploy",
  "installCommand": "npm install"
}