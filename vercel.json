{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/server.js" },
    { "src": "/health", "dest": "/server.js" },
    { "src": "/test", "dest": "/server.js" },
    { "src": "/diagnostic", "dest": "/server.js" },
    { "src": "/(.*)", "dest": "/server.js" }
  ]
}